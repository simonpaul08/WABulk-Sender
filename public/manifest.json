{
    "manifest_version": 3,
    "name": "WABulk Sender",
    "version": "2.0",
    "description": "Bulk Message Sender",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}